{
  "release-type": "simple",
  "prerelease-type": "rc",
  "versioning-strategy": "prerelease",
  "separate-pull-requests": false,
  "draft": false,
  "skip-github-release": true,
  "pull-request-title-pattern": "chore${scope}: release${component} ${version}",
  "prerelease": true,
  ".": "0.0.0"
}
